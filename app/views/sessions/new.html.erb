<div class="container">
<h1 class="page-header">Log in</h1>

<%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email],class:"form-control",placeholder:"enter your email" %>
  </div>
  <div class="form-group">
    <%= label_tag :password %><br />
    <%= password_field_tag :password,'', class:"form-control",placeholder:"enter your password..." %>
  </div>
 <%= submit_tag "Log in" ,class:"btn btn-success"%>
<% end %>
</div>
