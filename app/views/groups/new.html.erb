<div class="container">

<%= form_for :group ,url:groups_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :topicname %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <%= f.file_field :avatar %>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
</div>
